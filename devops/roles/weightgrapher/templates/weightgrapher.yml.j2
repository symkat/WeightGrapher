---
database:
  weightgrapher: postgresql://{{ database.weightgrapher.user }}:{{ database.weightgrapher.pass }}@{{ database.weightgrapher.host }}/weightgrapher

secrets:
{% for secret in secrets %}
  - {{ secret }}
{% endfor %}

smtp:
  host: {{ smtp.host }} 
  port: {{ smtp.port }}
  sasl_username: {{ smtp.sasl_username }}
  sasl_password: {{ smtp.sasl_password }}

mkit_path: {{ mkit_path }}
